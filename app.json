{
  "expo": {
    "name": "tranzipper-ui-v1",
    "slug": "tranzipper-ui-v1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "com.tranzipper.com",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "splash": {
      "image": "./assets/pngs/tranzipper_main.png",
      "backgroundColor": "#0A0A0A"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
